eturn {\n                ...state,\n                pet: action.pet\n            }\n\n        default:\n            return state\n    }\n}\n\nexport default petsReducer","/Users/yixuanliao/sarry94118/2021/spring/02/petfinder/login:register/petfinder-project/src/components/grid/pet-card-main-page.js",["271","272","273","274"],"import React, {useState} from 'react'\nimport petsReducer from \"../../reducer/pet-reducer\";\nimport {combineReducers, createStore} from \"redux\";\nimport {Link, useParams, useHistory} from \"react-router-dom\";\nimport PetCard from \"../grid/pet-card\"\nimport {Provider} from \"react-redux\";\n\nconst reducer = combineReducers({\n    petsReducer: petsReducer,\n})\n\nconst store = createStore(reducer)\n\n\nconst PetCardMainPage = () => {\n    // const [history] = useHistory()\n\n    return (<Provider store={store}>\n        <div>\n            {/*<Link to={`/profile`}>*/}\n            {/*    <a className=\"fas fa-arrow-left fa-2x text-dark float-md-right\">Back</a>*/}\n            {/*</Link>*/}\n\n            {/*<a className=\"fas fa-arrow-left fa-2x text-dark float-md-right\" onClick={() => history.goBack()}>Back</a>*/}\n            <PetCard/>\n            {/*<PetCard/>*/}\n\n        </div>\n    </Provider>)\n}\n\n\nexport default PetCardMainPage","/Users/yixuanliao/sarry94118/2021/spring/02/petfinder/login:register/petfinder-project/src/components/search/home-screen.js",[],"/Users/yixuanliao/sarry94118/2021/spring/02/petfinder/login:register/petfinder-project/src/components/search/search-screen.js",["275","276","277","278","279","280","281","282","283","284","285","286","287","288"],"import React, {useState, useEffect, useContext} from 'react'\nimport {Link, useParams, useHistory} from 'react-router-dom'\nimport petService from \"../../services/search-services/pet-service\"\nimport petSelfService from \"../../services/search-services/self-created-pet-service\"\nimport {AuthContext} from \"../../App\"\nconst petFinderKey = \"IERoPwTvvsgAtqgT7